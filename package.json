{
  "name": "smolbot",
  "version": "1.0.0",
  "description": "A procedurally generated 2D platformer built with Phaser 3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "stop": "node scripts/dev/stop-server.js",
    "restart": "npm run stop && npm run dev",
    "test:agent": "node scripts/agents/runner.js",
    "organize-assets": "node scripts/assets/organize.js",
    "organize-assets:dry-run": "node scripts/assets/organize.js --dry-run",
    "watch-assets": "node scripts/assets/watcher.js",
    "improve": "node scripts/improvements/manager.js",
    "improve:auto": "node scripts/agents/improvement-runner.js run",
    "assets:find": "node scripts/agents/asset-discovery.js search",
    "assets:generate": "node scripts/assets/local-generator.js generate",
    "assets:generate:batch": "node scripts/assets/local-generator.js batch",
    "audit-assets": "node scripts/assets/audit.js",
    "audit-assets:clean": "node scripts/assets/audit.js --clean",
    "save-progress": "node scripts/dev/progress.js",
    "progress:list": "node scripts/dev/progress.js list"
  },
  "keywords": [
    "phaser",
    "game",
    "platformer",
    "procedural-generation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chalk": "^5.6.2",
    "generate-maze": "^1.1.0",
    "ora": "^9.3.0",
    "phaser": "^3.90.0",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0",
    "simplex-noise": "^4.0.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "adm-zip": "^0.5.16",
    "chokidar": "^5.0.0",
    "playwright": "^1.58.2",
    "vite": "^7.3.1"
  }
}
